  <differential>
    <element id="DeviceAlert">
      <path value="DeviceAlert"/>
      <short value="Measurement, calculation or setting capability of a medical device"/>
      <definition value="Describes a measurement, calculation or setting capability of a device."/>
      <comment value="The DeviceAlert resource is derived from the ISO/IEEE 11073-10201 Domain Information Model standard, but is more widely applicable."/>
      <min value="0"/>
      <max value="*"/>
      <mapping>
        <identity value="w5"/>
        <map value="administrative.device"/>
      </mapping>
    </element>
    <element id="DeviceAlert.identifier">
      <path value="DeviceAlert.identifier"/>
      <short value="Instance identifier"/>
      <definition value="Instance identifiers assigned to a device, by the device or gateway software, manufacturers, other organizations or owners. For example, handle ID."/>
      <comment value="For identifiers assigned to a device by the device or gateway software, the `system` element of the identifier should be set to any identifier of the device."/>
      <min value="0"/>
      <max value="*"/>
      <type>
        <code value="Identifier"/>
      </type>
      <isSummary value="true"/>
      <mapping>
        <identity value="w5"/>
        <map value="FiveWs.identifier"/>
      </mapping>
    </element>
    <element id="DeviceAlert.type">
      <path value="DeviceAlert.type"/>
      <short value="Identity of Alert, for example Heart Rate or PEEP Setting"/>
      <definition value="Describes the type of the Alert. For example: Heart Rate, PEEP Setting, etc."/>
      <comment value="DeviceAlert.type can be referred to either IEEE 11073-10101 or LOINC."/>
      <min value="1"/>
      <max value="1"/>
      <type>
        <code value="string"/>
      </type>
      <isSummary value="true"/>
         <mapping>
        <identity value="w5"/>
        <map value="FiveWs.class"/>
      </mapping>
    </element>
    
    <element id="DeviceAlert.device">
      <path value="DeviceAlert.device"/>
      <short value="Describes the link to the device originating te alert"/>
      <definition value="Describes the link to the Device.  This is also known as a channel device."/>
      <comment value="Note if this is not the top level device - also know as the source device, it may be referenced in the device.parent element."/>
      <min value="1"/>
      <max value="1"/>
      <type>
        <code value="string"/>
        
      </type>
      <isSummary value="true"/>
    </element>
    <element id="DeviceAlert.operationalStatus">
      <path value="DeviceAlert.operationalStatus"/>
      <short value="on | off | standby | entered-in-error"/>
      <definition value="Indicates current operational state of the device. For example: On, Off, Standby, etc."/>
      <min value="0"/>
      <max value="1"/>
      <type>
        <code value="string"/>
      </type>
      <isSummary value="true"/>
      </element>
    
   
    
    
    
    
