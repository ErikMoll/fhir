<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="Resource-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-content"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-content"/>
        <description value="Search on the entire content of the resource"/>
        <code value="_content"/>
        <type value="string"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-text"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-text"/>
        <description value="Search on the narrative text (html) of the resource"/>
        <code value="_text"/>
        <type value="string"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-id"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Resource.id"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-id"/>
        <description value="Logical id of this artifact"/>
        <code value="_id"/>
        <type value="token"/>
        <expression value="Resource.id"/>
        <xpath value="f:Resource/f:id"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-lastUpdated"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Resource.meta.lastUpdated"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-lastUpdated"/>
        <description value="When the resource version last changed"/>
        <code value="_lastUpdated"/>
        <type value="date"/>
        <expression value="Resource.meta.lastUpdated"/>
        <xpath value="f:Resource/f:meta/f:lastUpdated"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-list"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-list"/>
        <description value="All resources in nominated list (by id, Type/id, url or one of the magic List types)"/>
        <code value="_list"/>
        <type value="string"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-profile"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Resource.meta.profile"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-profile"/>
        <description value="Profiles this resource claims to conform to"/>
        <code value="_profile"/>
        <type value="uri"/>
        <expression value="Resource.meta.profile"/>
        <xpath value="f:Resource/f:meta/f:profile"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-query"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-query"/>
        <description value="A custom search profile that describes a specific defined query operation"/>
        <code value="_query"/>
        <type value="token"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-security"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Resource.meta.security"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-security"/>
        <description value="Security Labels applied to this resource"/>
        <code value="_security"/>
        <type value="token"/>
        <expression value="Resource.meta.security"/>
        <xpath value="f:Resource/f:meta/f:security"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-source"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Resource.meta.source"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-source"/>
        <description value="Identifies where the resource comes from"/>
        <code value="_source"/>
        <type value="uri"/>
        <expression value="Resource.meta.source"/>
        <xpath value="f:Resource/f:meta/f:source"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-tag"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Resource.meta.tag"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-tag"/>
        <description value="Tags applied to this resource"/>
        <code value="_tag"/>
        <type value="token"/>
        <expression value="Resource.meta.tag"/>
        <xpath value="f:Resource/f:meta/f:tag"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-has"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-has"/>
        <description value="Provides limited support for reverse chaining - that is, selecting resources based on the properties of resources that refer to them (instead of chaining where resources can be selected based on the properties of resources that they refer to). See the FHIR search page for further documentation"/>
        <code value="_has"/>
        <type value="string"/>
        <xpathUsage value="other"/>
      </SearchParameter>
    </resource>
  </entry>  
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-type"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-type"/>
        <description value="Used when a search is performed in a context which doesn't limit the search to indicate which types are being searched. See the FHIR search page for further discussion"/>
        <code value="_type"/>
        <type value="token"/>
        <xpathUsage value="other"/>
      </SearchParameter>
    </resource>
  </entry>  
  <entry>
    <resource>
      <SearchParameter>
        <id value="Resource-filter"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Resource-filter"/>
        <description value="Filter search parameter which supports a more sophisticated grammar for searching. See [documentation](search_filter.html) for further details"/>
        <code value="_filter"/>
        <type value="token"/>
        <xpathUsage value="other"/>
      </SearchParameter>
    </resource>
  </entry>  
</Bundle>