<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="CareTeam-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="CareTeam-category"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="CareTeam.category"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/CareTeam-category"/>
        <description value="Type of team"/>
        <code value="category"/>
        <type value="token"/>
        <expression value="CareTeam.category"/>
        <xpath value="f:CareTeam/f:category"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="CareTeam-date"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="CareTeam.period"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/CareTeam-date"/>
        <description value="Time period team covers"/>
        <code value="date"/>
        <type value="date"/>
        <expression value="CareTeam.period"/>
        <xpath value="f:CareTeam/f:period"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="CareTeam-encounter"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="CareTeam.encounter"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/CareTeam-encounter"/>
        <description value="Encounter created as part of"/>
        <code value="encounter"/>
        <type value="reference"/>
        <expression value="CareTeam.encounter"/>
        <xpath value="f:CareTeam/f:encounter"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="CareTeam-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="CareTeam.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/CareTeam-identifier"/>
