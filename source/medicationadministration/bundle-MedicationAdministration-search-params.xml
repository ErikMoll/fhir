<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="MedicationAdministration-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="MedicationAdministration-code"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="MedicationAdministration.medicationCodeableConcept"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/MedicationAdministration-code"/>
        <description value="Return administrations of this medication code"/>
        <code value="code"/>
        <type value="token"/>
        <expression value="(MedicationAdministration.medication as CodeableConcept)"/>
        <xpath value="f:MedicationAdministration/f:medicationCodeableConcept"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="MedicationAdministration-context"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="MedicationAdministration.context"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/MedicationAdministration-context"/>
        <description value="Return administrations that share this encounter or episode of care"/>
        <code value="context"/>
        <type value="reference"/>
        <expression value="MedicationAdministration.context"/>
        <xpath value="f:MedicationAdministration/f:context"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
