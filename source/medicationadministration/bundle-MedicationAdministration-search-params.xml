          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="MedicationAdministration.effective[x]"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/MedicationAdministration-effective-time"/>
        <description value="Date administration happened (or did not happen)"/>
