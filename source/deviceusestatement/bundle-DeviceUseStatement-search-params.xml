<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="DeviceUseStatement-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceUseStatement-device"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceUseStatement.device"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceUseStatement-device"/>
        <description value="Search by device"/>
        <code value="device"/>
        <type value="reference"/>
        <expression value="DeviceUseStatement.device"/>
        <xpath value="f:DeviceUseStatement/f:device"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceUseStatement-identifier"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceUseStatement.identifier"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceUseStatement-identifier"/>
        <description value="Search by identifier"/>
        <code value="identifier"/>
        <type value="token"/>
        <expression value="DeviceUseStatement.identifier"/>
        <xpath value="f:DeviceUseStatement/f:identifier"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceUseStatement-patient"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceUseStatement.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceUseStatement-patient"/>
        <description value="Search by subject - a patient"/>
        <code value="patient"/>
        <type value="reference"/>
        <expression value="DeviceUseStatement.subject.where(resolve() is Patient)"/>
        <xpath value="f:DeviceUseStatement/f:subject"/>
        <xpathUsage value="normal"/>
        <target value="Patient"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="DeviceUseStatement-subject"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="DeviceUseStatement.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/DeviceUseStatement-subject"/>
        <description value="Search by subject"/>
        <code value="subject"/>
        <type value="reference"/>
        <expression value="DeviceUseStatement.subject"/>
        <xpath value="f:DeviceUseStatement/f:subject"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
</Bundle>