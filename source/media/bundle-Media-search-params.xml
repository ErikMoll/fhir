<?xml version="1.0" encoding="UTF-8"?>

<Bundle xmlns="http://hl7.org/fhir">
  <id value="Media-search-params"/>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Media-based-on"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Media.basedOn"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Media-based-on"/>
        <description value="Procedure that caused this media to be created"/>
        <code value="based-on"/>
        <type value="reference"/>
        <expression value="Media.basedOn"/>
        <xpath value="f:Media/f:basedOn"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
  <entry>
    <resource>
      <SearchParameter>
        <id value="Media-created"/>
        <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-standards-status">
          <valueCode value="trial-use"/>
        </extension>
        <extension url="http://hl7.org/fhir/build/StructureDefinition/path">
          <valueString value="Media.created[x]"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Media-created"/>
        <description value="When Media was collected"/>
        <code value="created"/>
        <type value="date"/>
        <expression value="Media.created"/>
        <xpath value="f:Media/f:createdDateTime | f:Media/f:createdPeriod"/>
        <xpathUsage value="normal"/>
      </SearchParameter>
    </resource>
  </entry>
