          <valueString value="Media.subject"/>
        </extension>
        <url value="http://hl7.org/fhir/build/SearchParameter/Media-patient"/>
        <description value="Who/What this Media is a record of"/>
        <code value="patient"/>
        <type value="reference"/>
        <expression value="Media.subject.where(resolve() is Patient)"/>
        <xpath value="f:Media/f:subject"/>
        <xpathUsage value="normal"/>
        <target value="Patient"/>
      </SearchParameter>
    </resource>
  </entry>
